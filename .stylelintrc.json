{
  "extends": "stylelint-config-standard",
  "plugins": [
    "stylelint-order",
    "stylelint-scss"
  ],
  "rules": {
    "at-rule-no-unknown": null,
    "scss/at-rule-no-unknown": true,
    "no-missing-end-of-source-newline": false,
    "order/properties-alphabetical-order": true
  },
  "ignoreFiles": []
}

